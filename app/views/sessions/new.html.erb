<h1>Log In</h1>

<p> <%= flash[:message] if flash[:message] %> </p>

<%= form_for :user do |f| %>

<%= f.label :username, 'Username:'%>
  <%= f.text_field :username %> <br>

  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password%> <br>
  <br>

  <%= f.submit 'Log In'%>

<% end %>


