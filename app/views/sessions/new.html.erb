<h1>Log In</h1>


<%= form_for :user do |f| %>

<%= f.label :username, 'Username:'%>
  <%= f.text_field :username %> <br>

  <%= f.label :password, 'Password:' %>
  <%= f.password_field :password%> <br>
  <br>

  <%= f.submit 'Log In'%>

<% end %>
<h3>Or: <%= button_to 'Log In With Google', '/auth/google_oauth2' %></h3>
